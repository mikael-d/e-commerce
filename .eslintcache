n      <Modal.Body>\n        <Container fluid>\n          <Row>\n            <Col md={4}>\n              <Image src={product.image} fluid />\n            </Col>\n            <Col md={8}>\n              <Row md={4}>\n                Lors du développement d’un projet, il arrive souvent de partir\n                sur de mauvaises bases en termes d’indentation et de normes de\n                code. Pour y remédier, il existe des outils qui permettent de\n                palier à ce genre de désagrément.\n                {' '}\n              </Row>\n              <Row md={4}>{`${product.price} € /${product.unit}`}</Row>\n              <Row md={4}>\n                <div className=\"modal-counter\">\n                  <button onClick={() => decreaseCount()}>-</button>\n                  <span>{count}</span>\n                  <button onClick={() => increaseCount()}>+</button>\n                </div>\n              </Row>\n            </Col>\n          </Row>\n        </Container>\n      </Modal.Body>\n      <Modal.Footer>\n        <Button variant=\"secondary\" onClick={handleClose}>\n          Close\n        </Button>\n        <Button\n          variant=\"primary\"\n          onClick={() => {\n            setCartCount(cartCount + count);\n            add(product, count);\n          }}\n        >\n          Understood\n        </Button>\n      </Modal.Footer>\n    </Modal>\n  );\n};\n\nconst CardProduct = ({\n  product,\n  cartCount,\n  setCartCount,\n\n}) => {\n  const [show, setShow] = useState(false);\n\n  const handleClose = () => setShow(false);\n  const handleShow = () => setShow(true);\n\n  const [count, setCount] = useState(1);\n\n  return (\n    <Col sm={4}>\n      <Card className=\"card\" fluid>\n        <Card.Img\n          className=\"card__image\"\n          variant=\"top\"\n          src={product.image}\n          alt={product.name}\n          fluid\n        />\n        <Card.Body className=\"card__body\">\n          <Card.Title>{product.name}</Card.Title>\n          <Card.Text>\n            {product.price}\n            euros/\n            {product.unit}\n          </Card.Text>\n          <Button\n            variant=\"primary\"\n            onClick={() => {\n              handleShow();\n            }}\n          >\n            Go somewhere\n          </Button>\n        </Card.Body>\n        <ProductModal\n          addToCart={addToCart}\n          setCount={setCount}\n          count={count}\n          product={product}\n          show={show}\n          handleClose={handleClose}\n          cartCount={cartCount}\n          setCartCount={setCartCount}\n        />\n      </Card>\n    </Col>\n  );\n};\n","/home/etudiant/Bureau/react-training-project/ecommerce/.eslintrc.js",[],"/home/etudiant/Bureau/react-training-project/ecommerce/src/App.test.js",["96"],"/home/etudiant/Bureau/react-training-project/ecommerce/src/setupTests.js",[],"/home/etudiant/Bureau/react-training-project/ecommerce/src/pages/Home/index.js",[],"/home/etudiant/Bureau/react-training-project/ecommerce/src/pages/Cart/index.js",["97"],"/* eslint-disable no-restricted-syntax */\n/* eslint-disable no-const-assign */\n/* eslint-disable no-unused-expressions */\n/* eslint-disable react/prop-types */\n/* eslint-disable no-unreachable */\n/* eslint-disable no-unused-vars */\n/* eslint-disable react/button-has-type */\n/* eslint-disable no-undef */\n/* eslint-disable react/jsx-filename-extension */\nimport React, { useState } from 'react';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport {\n  Row, Col, Container, Button, Table, Image,\n} from 'react-bootstrap';\n\nimport { ImCross } from 'react-icons/im';\nimport { updateCart, removeFromCart } from '../../actions/actions';\n\nimport './style.scss';\n\nconst CartPage = () => (\n  <Container className=\"cart-page\">\n    <Row>\n      <Col md={9}>\n        <ItemProduct />\n      </Col>\n      <Col>\n        <OrderSummary />\n      </Col>\n    </Row>\n  </Container>\n);\n\nexport default CartPage;\n\nconst OrderSummary = () => {\n  const items = useSelector((state) => state.items);\n  // const [subTotal, setSubTotal] = useState();\n\n  const subTotalList = items.map((item) => item.quantity * item.details.price);\n  let subTotal = 0;\n\n  for (const total of subTotalList) {\n    subTotal += total;\n  }\n  const shipping = items.length && subTotal > 0 ? 10 : 0;\n\n  return (\n    <Container className=\"order-summary\">\n      <Container className=\"order-summary__price\">\n        <Row className=\"order-summary--row\">Order Summary</Row>\n        <Row className=\"order-summary--row\">\n          <Container className=\"order-summary__price__sub-total\">\n            <Row>\n              <Col>Subtotal</Col>\n              <Col>\n                {subTotal.toFixed(2)}\n                €\n              </Col>\n            </Row>\n            <Row>\n              <Col>shipping</Col>\n              <Col>\n                {shipping.toFixed(2)}\n                €\n              </Col>\n            </Row>\n          </Container>\n        </Row>\n        <Row className=\"order-summary--row\">\n          <Col>Total</Col>\n          <Col>\n            {(subTotal + shipping).toFixed(2)}\n            €\n          </Col>\n        </Row>\n      </Container>\n      <Link to=\"/checkout\">\n        <Button className=\"order-summary__button\" disable={!!items.length}>\n          Checkout\n        </Button>\n      </Link>\n    </Container>\n  );\n};\n\nconst ItemProduct = () => {\n  const TableRow = ({ item }) => {\n    const dispatch = useDispatch();\n\n    const { quantity, details } = item;\n\n    // modifie la quantité pour un produit donné dans le state global\n\n    const updateItemQuantity = (product, qty) => {\n      dispatch(updateCart(product, qty));\n    };\n\n    // supprime l'article en question dans le state global\n    const removeItemFromCart = (product) => {\n      dispatch(removeFromCart(product));\n    };\n\n    const [itemQuantity, setItemQuantity] = useState(quantity);\n\n    console.log('itemQuantity initital', itemQuantity);\n\n    return (\n      <tr className=\"table-row\">\n        <td className=\"table-row__data\">\n          <Image src={details.image} fluid width={70} height={70} />\n        </td>\n        <td className=\"table-row__data\">{details.name}</td>\n        <td className=\"table-row__data\">{details.price}</td>\n        <td className=\"table-row__data\">\n          <div className=\"modal-counter\">\n            <button onClick={() => {\n              updateItemQuantity(item, quantity > 0 ? itemQuantity - 1 : itemQuantity);\n            }}\n            >\n              -\n            </button>\n            <span>{quantity}</span>\n            <button onClick={() => {\n              updateItemQuantity(item, quantity + 1);\n            }}\n            >\n              +\n            </button>\n          </div>\n        </td>\n        <td className=\"table-row__data\">\n          {(quantity * details.price).toFixed(2)}\n        </td>\n        <td className=\"table-row__data\">\n          <Button>\n            <ImCross onClick={() => removeItemFromCart(item)} />\n          </Button>\n        </td>\n      </tr>\n    );\n  };\n\n  const items = useSelector((state) => state.items);\n\n  return (\n    <Table responsive striped borderless hover variant=\"dark\">\n      <thead>\n        <tr>\n          <th>Product</th>\n          <th>Reference</th>\n          <th>Price</th>\n          <th>Quantity</th>\n          <th>Total</th>\n        </tr>\n      </thead>\n      <tbody>\n        {items.map((item) => (<TableRow item={item} />))}\n      </tbody>\n    </Table>\n  );\n};\n",["98","99","100","101","102","103"],"/home/etudiant/Bureau/react-training-project/ecommerce/src/actions/actions.js",[],"/home/etudiant/Bureau/react-training-project/ecommerce/src/ store.js",[],"/home/etudiant/Bureau/react-training-project/ecommerce/src/reducer.js",[],"/home/etudiant/Bureau/react-training-projec